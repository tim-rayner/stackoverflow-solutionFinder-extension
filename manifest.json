{
  "manifest_version": 2,
  "name": "Stack Overflow Solution Finder",
  "version": "1.0",
  "description": "Find the most upvoted answer on Stack Overflow.",
  "permissions": ["tabs", "https://*.stackoverflow.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.stackoverflow.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
